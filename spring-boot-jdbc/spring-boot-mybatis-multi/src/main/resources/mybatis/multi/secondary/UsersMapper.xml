<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.spring.boot.project.multi.mapper.secondary.MultiUsersMapper">

    <resultMap id="userResult" type="com.spring.boot.project.model.User">
        <id column="user_id" property="id" jdbcType="BIGINT"/>
        <result column="user_code" property="userCode" jdbcType="VARCHAR"/>
        <result column="user_name" property="userName" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="userColumns">
        user_id
        , user_Code, user_name
    </sql>

    <select id="findAll" parameterType="String" resultMap="userResult">
        SELECT *
        FROM users
    </select>
</mapper>